{
  "name": "zazu-system",
  "version": "1.0.0",
  "description": "Zazu plugin for for controlling totally random stuff like Bluetooth, and that's it for now.",
  "blocks": {
    "input": [
      {
        "id": "zazu-reload",
        "type": "Keyword",
        "keyword": "reload",
        "title": "Reload Zazu Config",
        "icon": "fa-sync",
        "connections": ["Reload"]
      }
    ],
    "output": [
      {
        "id": "Copy",
        "type": "CopyToClipboard",
        "text": "{value}"
      },
      {
        "id": "Reload",
        "type": "ReloadConfig"
      }
    ]
  }
}
